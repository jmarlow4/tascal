{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "mapRoot": "/",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "outDir": "../dist/",
    "rootDir": ".",
    "sourceMap": true,
    "target": "es5",
    "inlineSources": true
  },
  "types": [
    "firebase",
    "jasmine"
  ],
  "files": [
    "node_modules/angularfire2/angularfire2.d.ts",
    "node_modules/angularfire2/auth/auth.d.ts",
    "node_modules/firebase/firebase.d.ts",
    "main.ts",
    "typings.d.ts"
  ]
}
